    "@tsconfig/strictest": "^2.0.1",
    "@types/jscodeshift": "^0.12.0",
    "@types/node": "^22.6.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^2.0.5",
    "happy-dom": "^16.3.0",
    "lefthook": "^1.7.16",
    "semantic-release": "^24.2.0",
